{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bundle:size": "source-map-explorer 'build/static/js/*.js'",
    "start:local": "dotenv -f .env.localdev craco start",
    "start": "dotenv -f .env.staging craco start",
    "build": "craco build",
    "deploy": "curl -Is https://api.vercel.com/v1/integrations/deploy/QmWaNe4KAPWftq1YR9xdqS1L5GZB6NG7cMKYiPmaCouhgp/MSUGZZ8EOd | head -1",
    "deploy:production": "git checkout release && git reset --hard master && git push origin release",
    "storybook": "start-storybook -p 6006 -s public",
    "e2e": "yarn cypress open",
    "build-storybook": "build-storybook -s public",
    "fetch:scheme": "gq https://expane.hasura.app/v1/graphql -H \"X-Hasura-Admin-Secret: 12345678910\" --introspect > src/services/schema.graphql"
  },
  "dependencies": {
    "@tailwindcss/forms": "0.2.1",
    "@tailwindcss/postcss7-compat": "2.0.3",
    "autoprefixer": "9.8.6",
    "date-fns": "2.19.0",
    "firebase": "8.3.0",
    "graphql": "15.5.0",
    "graphql-request": "3.4.0",
    "mobx": "6.1.8",
    "mobx-react-lite": "3.2.0",
    "postcss": "7.0.35",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "6.15.4",
    "react-icons": "4.2.0",
    "react-query": "3.12.1",
    "react-router-dom": "5.2.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "6.1.1",
    "env-cmd": "10.1.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "@storybook/addon-actions": "^6.1.16",
    "@storybook/addon-essentials": "^6.1.16",
    "@storybook/addon-links": "^6.1.16",
    "@storybook/node-logger": "^6.1.16",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.21",
    "@types/node": "14.14.34",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/react-router-dom": "5.1.7",
    "cypress": "6.6.0",
    "react-scripts": "4.0.3",
    "source-map-explorer": "2.5.2",
    "typescript": "4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write"
    ]
  }
}
